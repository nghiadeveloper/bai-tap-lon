package baitaplon01;

public class baitap03 {

	public static void main(String[] args) {
		
		
        System.out.print("Nhập vào một số nguyên dương có 2 ký số (10-99): ");
        int n = scanner.nextInt();
        

        // Tính tổng của 2 ký số
        int firstDigit = n / 10; // Lấy ký số đầu tiên
        int secondDigit = n % 10; // Lấy ký số thứ hai
        int sum = firstDigit + secondDigit; // Tính tổng

        // Xuất kết quả
        System.out.println("Tổng của 2 ký số là: " + sum);

	}

}
